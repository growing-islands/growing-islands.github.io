{
  "name": "yume-lab_corporate-site",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:pathpida": "yarn pathpida:watch",
    "prebuild": "yarn pathpida:build",
    "build": "next build",
    "postbuild": "yarn generate:sitemap",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "pathpida:watch": "pathpida --watch",
    "pathpida:build": "pathpida --build",
    "eslint": "eslint src --fix",
    "stylelint": "stylelint 'src/**/*.scss' --fix",
    "generate:sitemap": "node ./sitemap.js",
    "format": "prettier --write .",
    "test": "jest --watch",
    "test-all": "yarn eslint && yarn format && yarn stylelint && yarn test",
    "analyze": "cross-env ANALYZE=true next build",
    "nojekyll": "touch docs/.nojekyll",
    "export": "yarn build && next export -o docs && yarn nojekyll"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "**/*.@(ts|tsx)": [
      "yarn eslint",
      "yarn stylelint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "@tailwindcss/postcss7-compat": "^2.2.16",
    "aos": "^2.3.4",
    "classnames": "^2.3.1",
    "destyle.css": "^3.0.0",
    "next": "^11.1.2",
    "pathpida": "^0.17.0",
    "react": "^17.0.2",
    "react-collapse": "^5.1.0",
    "react-dom": "^17.0.2",
    "react-google-recaptcha": "^2.1.0",
    "react-hook-form": "^7.19.1",
    "react-intersection-observer": "^8.32.2",
    "react-parallax": "^3.3.0",
    "react-redux": "^7.2.6",
    "react-scroll": "^1.8.4",
    "react-transition-group": "^4.4.2",
    "react-tsparticles": "^1.37.5",
    "react-use": "^17.3.1",
    "react-use-scroll-direction": "^0.1.0",
    "redux": "^4.1.1",
    "swiper": "^7.2.0",
    "tailwindcss": "^2.2.16",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@next/bundle-analyzer": "^11.1.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.3.0",
    "@types/aos": "^3.0.4",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-collapse": "^5.0.1",
    "@types/react-google-recaptcha": "^2.1.2",
    "@types/react-redux": "^7.1.20",
    "@types/react-scroll": "^1.8.3",
    "@types/react-transition-group": "^4.4.3",
    "@types/resize-observer-browser": "^0.1.6",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "autoprefixer": "^10.3.7",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.4",
    "jest-canvas-mock": "^2.3.1",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.0.10",
    "next-page-tester": "^0.29.0",
    "nextjs-sitemap-generator": "^1.3.1",
    "postcss": "^8.3.9",
    "prettier": "^2.0.2",
    "sass": "^1.42.1",
    "sass-loader": "^12.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-scss": "^1.0.0-security",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.21.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "typescript": "^4.1.3",
    "yarn-run-all": "^3.1.1"
  }
}
